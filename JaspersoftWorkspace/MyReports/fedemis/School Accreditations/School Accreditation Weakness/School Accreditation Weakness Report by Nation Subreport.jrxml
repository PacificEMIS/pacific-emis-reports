	<parameter name="Year" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[2019]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[select distinct t1.dName,t1.Standard,StandardName,concat(t1.Criteria,' : ',t1.CriteriaName) as Criteria, concat('Score ',cast(t1.[Score 1] as nvarchar)) as 'Score', schNo, t1.schName  from(
						select 
								ssa.inspID,d.dName, ssa.schNo,sch.schName, Standard,StandardName, CriteriaName,Criteria,
								cast(sum((case 
								when RIGHT(Score,1)>5 then CEILING(Score) 
								when RIGHT(Score,1)<=5 then FLOOR(Score) else Score end)) as int)  as 'Score 1'
								from pInspectionRead.SchoolStandardAssessment ssa
										join Schools sch on ssa.schNo=sch.schNo
										join Islands i on sch.iCode=i.iCode
										join Districts d on d.dID=i.iGroup 
										join 
										(
											Select inspid
											,MAX(startDate) inspStart 
											from warehouse.Accreditations group by inspID
										)insp on ssa.inspID=insp.inspID
						
								where category='School Evaluation' and ssa.InspectionYear=$P{Year}
								group by ssa.inspID,d.dName, ssa.schNo,sch.schName,Standard,StandardName, CriteriaName,Criteria
								having 
								cast(sum((case 
								when RIGHT(Score,1)>5 then CEILING(Score) 
								when RIGHT(Score,1)<=5 then FLOOR(Score) else Score end)) as int)=1
)t1 

union all 

select distinct t1.dName,t1.Standard,StandardName,concat(t1.Criteria,' : ',t1.CriteriaName), concat('Score ',cast(t1.[Score 2] as nvarchar)) as 'Score', schNo, t1.schName  from(
						select 
								ssa.inspID,d.dName, ssa.schNo,sch.schName, Standard,StandardName, CriteriaName,Criteria,
								cast(sum((case 
								when RIGHT(Score,1)>5 then CEILING(Score) 
								when RIGHT(Score,1)<=5 then FLOOR(Score) else Score end)) as int)  as 'Score 2'
								from pInspectionRead.SchoolStandardAssessment ssa
										join Schools sch on ssa.schNo=sch.schNo
										join Islands i on sch.iCode=i.iCode
										join Districts d on d.dID=i.iGroup 
										join 
										(
											Select inspid
											,MAX(startDate) inspStart 
											from warehouse.Accreditations group by inspID
										)insp on ssa.inspID=insp.inspID
						
								where category='School Evaluation' and ssa.InspectionYear=$P{Year}
								group by ssa.inspID,d.dName, ssa.schNo,sch.schName,Standard,StandardName, CriteriaName,Criteria
								having 
								cast(sum((case 
								when RIGHT(Score,1)>5 then CEILING(Score) 
								when RIGHT(Score,1)<=5 then FLOOR(Score) else Score end)) as int)=2
)t1 

order by 2,5]]>
	</queryString>
