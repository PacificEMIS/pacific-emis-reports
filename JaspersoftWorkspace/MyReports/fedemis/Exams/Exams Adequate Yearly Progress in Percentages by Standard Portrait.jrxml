	<queryString language="SQL">
		<![CDATA[SELECT [examName]
      ,[State]
      ,[schName]
      ,CONCAT([standardCode], ': ', [standardDesc]) AS ExamStandard
	  ,[standardCode]
	  ,[standardDesc]
	  ,SUM(CASE WHEN examYear = $P{Year}-2 THEN ROUND((convert(float,ISNULL([3], 0)) + ISNULL([4], 0)) / Candidates,2) END) [Year1]
      ,SUM(CASE WHEN examYear = $P{Year}-1 THEN ROUND((convert(float,ISNULL([3], 0)) + ISNULL([4], 0)) / Candidates,2) END) [Year2]
	  ,SUM(CASE WHEN examYear = $P{Year} THEN ROUND((convert(float,ISNULL([3], 0)) + ISNULL([4], 0)) / Candidates,2) END) [Year3]   
FROM [warehouse].[ExamSchoolStandardsX]
INNER JOIN Schools ON [warehouse].[ExamSchoolStandardsX].schNo = Schools.schNo
WHERE examCode = $P{Exam}
GROUP BY [examName]
      ,[State]
      ,[schName]
	  ,[standardCode]
	  ,[standardDesc]
ORDER BY ExamStandard, State, schName]]>
	</queryString>
