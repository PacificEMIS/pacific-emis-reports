	</parameter>
	<queryString language="SQL">
		<![CDATA[select 
	distinct ISNULL(main.Standard,'N/A') AS Standard, 
	ISNULL(main.StandardName,'N/A') AS StandardName,
	isnull(concat(main.Criteria,' : ',main.CriteriaName),'N/A') AS Criteria,
	isnull(fnl1.totalSchools,0) as 'Level 1',
	isnull(fnl2.totalSchools,0) as 'Level 2' 
	from pInspectionRead.SchoolStandardAssessment main
	
left join
(	select ISNULL(t2.Standard,'N/A') AS Standardl1,ISNULL(t2.CriteriaName,'N/A') AS CriteriaNamel1,count(distinct t2.schNo) as totalSchools from (
								select 
										ssa.inspID,ssa.schNo,Standard, CriteriaName
										,cast(sum((case 
										when RIGHT(Score,1)>5 then CEILING(Score) 
										when RIGHT(Score,1)<=5 then FLOOR(Score) else Score end)) as int)  as 'Level 1'
										from pInspectionRead.SchoolStandardAssessment ssa
												join 
												(
													Select inspid
													,MAX(startDate) inspStart 
													from warehouse.Accreditations group by inspID
												)insp on ssa.inspID=insp.inspID
												join Schools sch on ssa.schNo=sch.schNo
												join Islands i on sch.iCode=i.iCode
												join Districts d on i.iGroup=d.dID
					
										where Category='Classroom Observation 1'
										and ssa.InspectionYear=$P{Year} and d.dID=$P{District}
										group by ssa.inspID,ssa.schNo,Standard, CriteriaName
										having 
										cast(sum((case 
										when RIGHT(Score,1)>5 then CEILING(Score) 
										when RIGHT(Score,1)<=5 then FLOOR(Score) else Score end)) as int)=1
		)t2 group by t2.Standard,t2.CriteriaName
)fnl1 on isnull(main.Standard,'N/A')=fnl1.Standardl1 and isnull(main.CriteriaName,'N/A')= fnl1.CriteriaNamel1

left join
(	select ISNULL(t2.Standard,'N/A') AS Standardl2,ISNULL(t2.CriteriaName,'N/A') AS CriteriaNamel2,count(distinct t2.schNo) as totalSchools from (
								select 
										ssa.inspID,ssa.schNo,Standard, CriteriaName
										,cast(sum((case 
										when RIGHT(Score,1)>5 then CEILING(Score) 
										when RIGHT(Score,1)<=5 then FLOOR(Score) else Score end)) as int)  as 'Level 2'
										from pInspectionRead.SchoolStandardAssessment ssa
												join 
												(
													Select inspid
													,MAX(startDate) inspStart 
													from warehouse.Accreditations group by inspID
												)insp on ssa.inspID=insp.inspID
												join Schools sch on ssa.schNo=sch.schNo
												join Islands i on sch.iCode=i.iCode
												join Districts d on i.iGroup=d.dID
					
										where Category='Classroom Observation 1'
										and ssa.InspectionYear=$P{Year} and d.dID=$P{District}
										group by ssa.inspID,ssa.schNo,Standard, CriteriaName
										having 
										cast(sum((case 
										when RIGHT(Score,1)>5 then CEILING(Score) 
										when RIGHT(Score,1)<=5 then FLOOR(Score) else Score end)) as int)=2
		)t2 group by t2.Standard,t2.CriteriaName
)fnl2 on 
	isnull(main.Standard,'N/A')=fnl2.Standardl2 and 
	isnull(main.CriteriaName,'N/A')= fnl2.CriteriaNamel2
where  main.Category='Classroom Observation 1' and fnl1.totalSchools>0 or fnl2.totalSchools>0 order by 4 desc,5 desc]]>
	</queryString>
