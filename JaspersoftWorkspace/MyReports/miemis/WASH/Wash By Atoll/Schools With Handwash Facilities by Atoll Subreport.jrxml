	<subDataset name="Dataset1" uuid="bd6d92fd-8274-4a44-a87a-c85dfdb01223">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="MIEMIS-OLD"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="Year" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[2019]]></defaultValueExpression>
		</parameter>
		<parameter name="District" class="java.lang.String">
			<defaultValueExpression><![CDATA["14"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[DECLARE @mainTable int;
set @mainTable=(select count(*) as secondaryTable from warehouse.TempWashCoreSurveyData
 where InspectionYear=$P{Year});

 IF @mainTable>0

BEGIN
   --------------------------------------Query 1----------------------------------------------------
select answer as 'ResourceAvailable',count(t2.Answer) as 'TotalSchool' from(
		select 
			distinct twc.schNo, cte.dname,
			twc.Answer,
			twc.inspStart from warehouse.TempWashCoreSurveyData as twc	
			join (
					Select d.dname, tcs.schNo, MAX(inspStart) inspStart from warehouse.TempWashCoreSurveyData tcs
					join Schools sch on tcs.schNo=sch.schNo
					join Islands i on sch.iCode=i.iCode
					join Districts d on i.iGroup=d.dID where  d.dID=$P{District} 
					and InspectionYear = $P{Year}  
					Group by tcs.schNo,d.dname, InspectionYear
				) cte on twc.schNo=cte.schNo
					  and twc.inspStart=cte.inspStart 
					  where twc.SubGroupName='Hygiene' and twc.answer in ('YES','NO') group by twc.schNo, cte.dName, Answer,twc.inspStart
)t2 group by t2.Answer order by answer desc
   ------------------------------------End of Query 1-----------------------------------------------
END
ELSE
BEGIN
       --------------------------------------Query 2----------------------------------------------------

select  resourceAvailable as 'ResourceAvailable',COUNT(resourceAvailable) as 'TotalSchool' from 
(
		select schNo, dname,resourceType,
						case when resourceAvailable=-1 then 'YES' else 'NO' end as resourceAvailable  from 
			(	select
						distinct ss.schNo,
						d.dName,
						ths.ssid
						, resSplit as resourceType
						,isnull(resAvail,0) as resourceAvailable from warehouse.TempHygieneSurvey ths
						join SchoolSurvey ss on ths.ssID=ss.ssID 
						join Schools sch on ss.schNo=sch.schNo
						join Islands i on sch.iCode=i.iCode
						join Districts d on i.iGroup=d.dID and d.dID=$P{District}
						where ss.svyYear=$P{Year} 
		)t1
)t2 group by resourceAvailable order by resourceAvailable desc

   ------------------------------------End of Query 2-----------------------------------------------
END]]>
		</queryString>
		<field name="ResourceAvailable" class="java.lang.String"/>
