	<queryString language="SQL">
		<![CDATA[SELECT ROUND(SUM([AvgDailyAtt]),0) AS AvgDailyAttTotal
      ,ROUND(SUM([AvgDailyMem]),0) AS AvgDailyMem
	  ,[InspQuarterlyReport]	
	  ,CASE SUBSTRING([InspQuarterlyReport],6,2)
        WHEN 'Q1' THEN cast(SUBSTRING([InspQuarterlyReport],1,4) AS VARCHAR(4)) + '-3'
        WHEN 'Q2' THEN cast(SUBSTRING([InspQuarterlyReport],1,4) AS VARCHAR(4)) + '-4'
        WHEN 'Q3' THEN cast(SUBSTRING([InspQuarterlyReport],1,4) AS VARCHAR(4)) + '-1'
        WHEN 'Q4' THEN cast(SUBSTRING([InspQuarterlyReport],1,4) AS VARCHAR(4)) + '-2'
    END AS inspQuarter  
  FROM [pInspectionRead].[QuarterlyReports]
  GROUP BY [InspQuarterlyReport]
  ORDER BY  [InspQuarterlyReport]]]>
	</queryString>
